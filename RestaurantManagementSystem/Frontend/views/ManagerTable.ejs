    <script>
        const fetchTableData = async () => {
            try {
                const response = await fetch('http://localhost:5000/GetTable', {
                    method: 'GET',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                });
                const responseFormat = await response.json();
                const tableData = responseFormat.data;
                renderTables(tableData);  // Call a function to render the blogs on the page
            } catch (err) {
                console.error(err);
                document.querySelector('.blog-item-box').innerHTML = '<p>Error fetching blog data.</p>';
            }
        };

        async function loadFoodOrders(Number) {
            const response = await fetch(`http://localhost:5000/GetOrder/${Number}`, {
                method: 'GET',
                headers: {
                    'Content-Type': 'application/json',
                },
            });

            const responseFormat=await response.json();
            const order= responseFormat.data;

            return order;
        }


        const renderTables =async(data) => {
            const tableContainer = document.querySelector('.blog-item-box .row');
            tableContainer.innerHTML = '';  // Clear any existing content
            
            for (const table of data) {
                let status=table.status;
                let order= await loadFoodOrders(table.tableNumber);
                console.log(order); // Now this will log the correct value
                const tableHTML = `
                    <div class="col-xl-4 col-lg-6">
                        <div class="home-blog-style-one-item">
                            <img src="data:image/jpeg;base64," alt="Image not Found">
                            <div class="content">
                                <div class="info">
                                    <div class="date"><strong>${table.tableNumber}</strong>Table</div>
                                    <ul class="blog-meta">
                                        <li>
                                            By <a href="#">Table ${table.tableNumber}</a>
                                        </li>
                                        <li>
                                            <a href="#">Status : ${status ? 'Active' : 'Inactive'}</a>
                                        </li>
                                        ${status 
                                        ? 
                                            `<li>
                                            <a href="#"><h6>Quantity : ${order.Total_Quantity}</h6></a>
                                            </li>` 
                                        :
                                            `<li>
                                            <a href="#"><h6>Quantity : -- </h6></a>
                                            </li>` 
                                        }
                                    </ul>
                                    ${status 
                                    ? 
                                        `<h6 class="title">
                                        <a href="#">Total Amount - Rs. ${order.Total_Amount}</a>
                                        </h6>
                                        <a href="ShowItems${table.tableNumber}" class="btn-read-more">Show Items<i class="fas fa-arrow-right"></i></a>` 
                                    :
                                        `<h6 class="title">
                                        <a href="#">Total Amount : --</a>
                                        </h6>
                                        <a href="ShowItems${table.tableNumber}" class="btn-read-more">No Items<i class="fas fa-arrow-right"></i></a>` 
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                `;
                tableContainer.insertAdjacentHTML('beforeend', tableHTML);
            };
        };

        fetchTableData();
    </script>

