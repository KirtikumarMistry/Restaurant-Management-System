    <script>
        // Function to update the count of a specific item
        const updateCount = (itemCountElement, increment) => {
            let count = parseInt(itemCountElement.textContent);
            count += increment;
            if (count < 0) count = 0; // Prevent negative count
            itemCountElement.textContent = count;
        };

        // Fetch item data and dynamically create the food items
        const fetchItemData = async () => {
            try {
                const response = await fetch('http://localhost:5000/Food-Menu/GetItem', {
                    method: 'GET',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                });
                const responseFormat = await response.json();
                const itemData = responseFormat.data;

                if (itemData && itemData.length > 0) {
                    const container = document.querySelector('.food-category-area .row');
                    container.innerHTML = ''; // Clear existing items
                    let totalcount = 0;

                    itemData.forEach((item) => {
                        // Create a new item div
                        const foodItemDiv = document.createElement('div');
                        foodItemDiv.classList.add('col-xl-3', 'col-lg-6', 'col-md-6');

                        // Create the HTML structure
                        foodItemDiv.innerHTML = `
                            <div class="food-category-item">
                                <div class="thumb">
                                    <img src="${item.image || 'assets/img/800x600.png'}" alt="Image Not Found">
                                </div>
                                <div class="info">
                                    <h4><a href="#">${item.name}</a></h4>
                                    <p>${item.description || 'No description available'}</p>
                                    <h5>Rs. ${item.price}</h5>
                                    <div class="cart-controls">
                                        <button class="cart-btn increase-btn">+</button>
                                        <span class="item-count" id="item-count-${totalcount}">0</span>
                                        <button class="cart-btn decrease-btn">-</button>
                                    </div>
                                </div>
                            </div>
                        `;
                        ++totalcount;

                        // Append the new item to the container
                        container.appendChild(foodItemDiv);
                    });

                    // After adding the items, attach event listeners to the increase and decrease buttons
                    document.querySelectorAll('.increase-btn').forEach((button, index) => {
                        button.addEventListener('click', () => {
                            const itemCountElement = document.getElementById(`item-count-${index}`);
                            updateCount(itemCountElement, 1);
                        });
                    });

                    document.querySelectorAll('.decrease-btn').forEach((button, index) => {
                        button.addEventListener('click', () => {
                            const itemCountElement = document.getElementById(`item-count-${index}`);
                            updateCount(itemCountElement, -1);
                        });
                    });
                }
            } catch (err) {
                console.error(err);
            }
        };

        // Call the function to fetch and display the items
        fetchItemData();
    </script>    
