    <script>
        // Get the current URL
        const currentUrl = window.location.href;

        // Use a regular expression to extract the number
        const match = currentUrl.match(/Food-Menu(\d+)/);
        const tableNum = match ? match[1] : null; // Extract the number or return null if no match

        console.log(tableNum); 

        document.getElementById('table-num').innerHTML=`Table : ${tableNum}`;
        
        document.getElementById('order-now-btn').addEventListener('click', async (e) => {
            e.preventDefault(); // Prevent default behavior of the link

            // Gather item data
            const items = [];
            document.querySelectorAll('.food-category-item').forEach((itemElement, index) => {
                const itemName = itemElement.querySelector('h4 a').textContent; // Get item name
                const itemCount = parseInt(document.getElementById(`item-count-${index}`).textContent); // Get item count
    
                if (itemCount > 0) { // Only send items with count > 0
                    items.push({ Name:itemName, Quantity: itemCount });
                }
            });
            if (items.length > 0) {
                try 
                {
                    // Send the data to the /AddItemOrdered endpoint
                    const response = await fetch('http://localhost:5000/AddOrder', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                        body: JSON.stringify({ Table_Number : tableNum ,Items_Ordered : items }), // Send the gathered items as JSON
                    });
                    const responseData = await response.json();
                    console.log(responseData.message);
    
                    if (responseData.success) {
                        console.log(responseData.order);
                        alert('Order placed successfully!');

                        // Redirect to the link provided in the href attribute
                        window.location.href = e.target.href; // This uses the existing link from the anchor tag
                    } else {
                        alert('Failed to place order.');
                    }
                } 
                catch (err) 
                {
                    console.error('Error:', err);
                    alert('An error occurred while placing the order.');
                }
            } else {
                alert('No items selected.');
            }
        });
    </script>
